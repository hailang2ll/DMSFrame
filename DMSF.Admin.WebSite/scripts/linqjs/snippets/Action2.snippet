<CodeSnippet Format="1.1.0" xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <Header>
    <Title>two args action</Title>
    <Author>neuecc</Author>
    <Shortcut>action2</Shortcut>
    <Description>one line two args action</Description>
    <SnippetTypes>
      <SnippetType>Expansion</SnippetType>
    </SnippetTypes>
  </Header>
  <Snippet>
    <Declarations>
      <Literal>
        <ID>arg1</ID>
        <Default>x</Default>
      </Literal>
      <Literal>
        <ID>arg2</ID>
        <Default>y</Default>
      </Literal>
    </Declarations>
    <Code Language="jscript"><![CDATA[function ($arg1$, $arg2$) { $end$ }]]></Code>
  </Snippet>
</CodeSnippet>